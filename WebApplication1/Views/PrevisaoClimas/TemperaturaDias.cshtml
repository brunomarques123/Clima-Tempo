

<div class="row">
    <div class="col-md-4">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item)
                    </td>
                </tr>
            }

        </table>
    </div>   
</div>
